{
  "expo": {
    "name": "ShortCutME",
    "slug": "ShortCutME",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3e73f0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.hit.shortcutme",
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "backgroundColor":"#3e73f0"
      },
      "permissions": []
    }
  }
}
